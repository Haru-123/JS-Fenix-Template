{
    "$schema": "https://raw.githubusercontent.com/acharluk/FenixDefaultTemplates/master/fenix-schema.json",
    "author": "adrestrod",
    "repoName": "MoreFenixTemplates",
    "repoUrl": "https://raw.githubusercontent.com/adrestrod/MoreFenixTemplates/master/",
    "version": 1.0,
    "templates": [
        {
            "id": "JavaScriptFenixTemplate",
            "displayName": "JavaScript",
            "language": "JavaScript",
            "categories": ["Programming"],
            "description": "A Simple JS template",
            "img": "https://upload.wikimedia.org/wikipedia/commons/thumb/9/99/Unofficial_JavaScript_logo_2.svg/1200px-Unofficial_JavaScript_logo_2.svg.png",
            "directories": [
                "js"
            ],
            "files": {
                "download": [
                    {
                        "from": "init-js.txt",
                        "to": "index.js"
                    }
                ],
                "open": [
                    "index.js"
                ]
            }
        },
        {
            "id": "WebPageFenixTemplate",
            "displayName": "WebPage(HTML+CSS+JS)",
            "language": "HTML",
            "categories": ["WebDesign"],
            "description": "A complete web page template for Fenix",
            "img": "https://www.google.com/url?sa=i&url=https%3A%2F%2Fwww.kindpng.com%2Fimgv%2FiihibxR_html-dialog-element-taking-full-advantage-of-javascript%2F&psig=AOvVaw0eX0vGRONGzhwh3xO8IdeL&ust=1584915433746000&source=images&cd=vfe&ved=0CAIQjRxqFwoTCJiRzp7MrOgCFQAAAAAdAAAAABAJ",
            "directories": [
                "HTMl",
                "CSS",
                "JS"
            ],
            "files": {
                "download": [
                    {
                        "from": "init-js.txt",
                        "to": "JS/index.js"
                    },
                    {
                        "from": "init-html.txt",
                        "to": "index.html"
                    },
                    {
                        "from": "init-css.txt",
                        "to": "CSS/index.css"
                    }
                ],
                "open": [
                    "index.js",
                    "index.html",
                    "index.css"
                ]
            }
        }
    ]
}
